{
    "name": "deltasystems/delta-cli",
    "bin": [
        "bin/delta",
        "bin/delta-docker.bat",
        "bin/delta-docker.ps1",
        "bin/delta-docker-build.bat",
        "bin/delta-docker-build.ps1",
        "bin/delta-docker-rebuild.bat",
        "bin/delta-docker-rebuild.ps1",
        "bin/delta-docker-generate-ssh-key.bat",
        "bin/delta-docker-generate-ssh-key.ps1"
    ],
    "autoload": {
        "psr-4": {
            "DeltaCli\\": "library/DeltaCli"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/console": ">=2.0",
        "symfony/stopwatch": ">=2.0",
        "cocur/slugify": "2.*",
        "guzzlehttp/guzzle": "~6.0",
        "react/react": "0.4.*",
        "react/child-process": "0.4.*",
        "paragonie/random_compat": "1.*"
    },
    "require-dev" : {
        "phpunit/dbunit": "2.*",
        "phpunit/phpunit": "4.*",
        "phpunit/phpunit-mock-objects": "2.*",
        "phpstan/phpstan": "^0.9.1",
        "jakub-onderka/php-parallel-lint": "^0.9.2"
    }
}
